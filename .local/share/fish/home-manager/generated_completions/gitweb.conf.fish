# gitweb.conf
complete -c 'gitweb.conf' -l export-all -d '$export_auth_hook'
complete -c 'gitweb.conf' -l syntax -d '<syntax> to "highlight"'
complete -c 'gitweb.conf' -o 'S<string>' -d 'option, which refers to pickaxe entry in gitdiffcore(7) for more details)'
complete -c 'gitweb.conf' -s l -d 'does; see description of'
complete -c 'gitweb.conf' -l author
complete -c 'gitweb.conf' -l committer -d and
complete -c 'gitweb.conf' -l grep -d 'options in git-log(1) manpage.  Enabled by default'
